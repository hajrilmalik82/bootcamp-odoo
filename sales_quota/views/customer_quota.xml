<odoo>
  <record id="action_customer_quota_usage" model="ir.actions.act_window">
    <field name="name">Customer Quota Usage</field>
    <field name="res_model">customer.quota</field>
    <field name="view_mode">tree,form</field>
  </record>

  <record id="view_customer_quota_tree" model="ir.ui.view">
    <field name="name">customer.quota.tree</field>
    <field name="model">customer.quota</field>
    <field name="arch" type="xml">
      <tree>
        <field name="partner_id"/>
        <field name="quota_category_id"/>
        <field name="year"/>
        <field name="month"/>
        <field name="quantity"/>
      </tree>
    </field>
  </record>
</odoo>
