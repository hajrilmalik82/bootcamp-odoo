<odoo>

  <!-- Tree View -->
  <record id="quota_category_tree" model="ir.ui.view">
    <field name="name">quota.category.tree</field>
    <field name="model">quota.category</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
      </tree>
    </field>
  </record>

  <!-- Form View -->
  <record id="quota_category_form" model="ir.ui.view">
    <field name="name">quota.category.form</field>
    <field name="model">quota.category</field>
    <field name="arch" type="xml">
      <form string="Quota Category">
        <sheet>
          <group>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <!-- Action -->
  <record id="action_quota_category" model="ir.actions.act_window">
    <field name="name">Quota Category</field>
    <field name="res_model">quota.category</field>
    <field name="view_mode">tree,form</field>
  </record>


</odoo>
