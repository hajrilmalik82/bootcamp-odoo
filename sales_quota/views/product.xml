<odoo>
  <record id="product_template_quota_form" model="ir.ui.view">
    <field name="name">product.template.quota.form</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view"/>
    <field name="arch" type="xml">
      <field name="categ_id" position="after">
        <field name="quota_category_id" string="category"/>
      </field>
    </field>
  </record>
</odoo>
