<odoo>

  <record id="view_quota_usage_tree" model="ir.ui.view">
    <field name="name">quota.usage.report.tree</field>
    <field name="model">quota.usage.report</field>
    <field name="arch" type="xml">
      <tree string="Global Quota Usage" create="false" edit="false" delete="false">
        <field name="year"/>
        <field name="month"/>
        <field name="allocated_qty" string="quantity"/>
        <field name="used_qty" string="Used"/>
        <field name="remaining_qty" string="Remaining"/>
      </tree>
    </field>
  </record>

  <record id="action_quota_usage" model="ir.actions.act_window">
    <field name="name">Global Quota Usage</field>
    <field name="res_model">quota.usage.report</field>
    <field name="view_mode">tree</field>
  </record>

</odoo>