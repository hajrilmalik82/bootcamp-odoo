<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_akademik" model="ir.module.category">
        <field name="name">Sistem Akademik</field>
        <field name="description">Manajemen Data Akademik</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_akademik_student" model="res.groups">
        <field name="name">Mahasiswa</field>
        <field name="category_id" ref="module_category_akademik"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_akademik_officer" model="res.groups">
        <field name="name">Academic Officer</field>
        <field name="category_id" ref="module_category_akademik"/>
        <field name="implied_ids" eval="[(4, ref('group_akademik_student'))]"/>
    </record>

    <record id="group_akademik_dosen" model="res.groups">
        <field name="name">Dosen</field>
        <field name="category_id" ref="module_category_akademik"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>
